    <!-- http://ada.filmontology.org/resource/2020/03/17/AnnotationType/DialogueVoiceQuality -->

    <owl:NamedIndividual rdf:about="http://ada.filmontology.org/resource/2020/03/17/AnnotationType/DialogueVoiceQuality">
