        <hasPredefinedValue rdf:resource="http://ada.filmontology.org/resource/2020/03/17/AnnotationValue/AspectRatio_1_1"/>
        <hasPredefinedValue rdf:resource="http://ada.filmontology.org/resource/2020/03/17/AnnotationValue/AspectRatio_21_9"/>
        <hasPredefinedValue rdf:resource="http://ada.filmontology.org/resource/2020/03/17/AnnotationValue/AspectRatio_4_3"/>
