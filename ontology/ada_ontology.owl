    <owl:NamedIndividual rdf:about="http://ada.filmontology.org/resource/2020/03/17/AnnotationValue/FacialExpressionsIntensity_5">
        <rdf:type rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationValue"/>
