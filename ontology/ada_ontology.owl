        <belongsToLevel rdf:resource="http://ada.filmontology.org/resource/2020/03/17/AnnotationLevel/ImageComposition"/>
        <hasPredefinedValue rdf:resource="http://ada.filmontology.org/resource/2020/03/17/AnnotationValue/SplitscreenDynamics_dynamic"/>
        <hasPredefinedValue rdf:resource="http://ada.filmontology.org/resource/2020/03/17/AnnotationValue/SplitscreenDynamics_static"/>
