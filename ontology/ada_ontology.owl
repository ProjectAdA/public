        <hasPredefinedValue rdf:resource="http://ada.filmontology.org/resource/2020/03/17/AnnotationValue/SoundGestureDynamics_pause"/>
        <hasPredefinedValue rdf:resource="http://ada.filmontology.org/resource/2020/03/17/AnnotationValue/SoundGestureDynamics_subside"/>
