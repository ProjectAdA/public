    <!-- http://ada.filmontology.org/resource/2020/03/17/AnnotationValue/ColourDesign_areal -->

    <owl:NamedIndividual rdf:about="http://ada.filmontology.org/resource/2020/03/17/AnnotationValue/ColourDesign_areal">
