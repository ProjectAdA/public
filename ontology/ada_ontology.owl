    <!-- http://ada.filmontology.org/resource/2020/03/17/AnnotationValue/CameraMovementSpeed_medium -->

    <owl:NamedIndividual rdf:about="http://ada.filmontology.org/resource/2020/03/17/AnnotationValue/CameraMovementSpeed_medium">
        <rdf:type rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationValue"/>
        <belongsToType rdf:resource="http://ada.filmontology.org/resource/2020/03/17/AnnotationType/CameraMovementSpeed"/>
        <annotationNumericValue rdf:datatype="http://www.w3.org/2001/XMLSchema#float">3.0</annotationNumericValue>
        <sequentialNumber>112</sequentialNumber>
        <rdfs:comment xml:lang="de">Die Kamerabewegung zeichnet sich im Verhältnis zu anderen Kamerabewegungen durch einen mittleren Grad an Geschwindigkeit aus, d.h. ist weder signifikant langsam oder schnell.</rdfs:comment>
        <rdfs:comment xml:lang="en">The camera movement can be characterised by a comparatively medium degree of speed, i.e. the camera moves neither significantly fast nor slow.</rdfs:comment>
        <rdfs:label xml:lang="en">medium</rdfs:label>
        <rdfs:label xml:lang="de">mittel</rdfs:label>
    </owl:NamedIndividual>
    


