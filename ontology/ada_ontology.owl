<?xml version="1.0"?>
<rdf:RDF xmlns="http://ada.filmontology.org/ontology/2020/03/17/"
     xml:base="http://ada.filmontology.org/ontology/2020/03/17/"
     xmlns:ao="http://ada.filmontology.org/ontology/2020/03/17/"
     xmlns:ar="http://ada.filmontology.org/resource/2020/03/17/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:sh="http://schema.org/"
     xmlns:dbo="http://dbpedia.org/ontology/"
     xmlns:j.0="http://purl.org/vocommons/voaf#"
     xmlns:j.1="http://purl.org/dc/dcmitype/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:dcterms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://ada.filmontology.org/ontology/2020/03/17/">
        <rdf:type rdf:resource="http://purl.org/vocommons/voaf#Vocabulary"/>
        <dc:creator>Henning Agt-Rickauer</dc:creator>
        <dcterms:description xml:lang="en">The AdA Ontology provides a vocabulary of film-analytical terms and concepts for fine-grained semantic video annotation.</dcterms:description>
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2017-04-28</dcterms:issued>
        <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-03-17</dcterms:modified>
        <dcterms:title>AdA Ontology - Ontology of Audio-Visual Rhetorics of Affect (Annotation Levels, Types, and Values)</dcterms:title>
        <vann:preferredNamespacePrefix>ao</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>http://ada.filmontology.org/ontology/2020/03/17/</vann:preferredNamespaceUri>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1.8</owl:versionInfo>
    </owl:Ontology>
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/annotationType -->

    <owl:ObjectProperty rdf:about="http://ada.filmontology.org/ontology/2020/03/17/annotationType">
        <rdfs:range rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationType"/>
        <rdfs:comment xml:lang="en">The annotation type property is used to associate an annotation type to a concrete movie annotation.</rdfs:comment>
        <rdfs:label xml:lang="en">annotationType</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/annotationValue -->

    <owl:ObjectProperty rdf:about="http://ada.filmontology.org/ontology/2020/03/17/annotationValue">
        <rdfs:domain rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/PredefinedValuesAnnotationType"/>
        <rdfs:range rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationValue"/>
        <rdfs:comment xml:lang="en">The annotation value property is used to associate a predefined value to a concrete movie annotation.</rdfs:comment>
        <rdfs:label xml:lang="en">annotationValue</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/annotationValueSequence -->

    <owl:ObjectProperty rdf:about="http://ada.filmontology.org/ontology/2020/03/17/annotationValueSequence">
        <rdfs:domain rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/ContrastingAnnotationType"/>
        <rdfs:domain rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/EvolvingAnnotationType"/>
        <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
        <rdfs:comment xml:lang="en">The annotation value sequence property is used to associate an ordered collection of annotation values to a concrete movie annotation.</rdfs:comment>
        <rdfs:label xml:lang="en">annotationValueSequence</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/belongsToLevel -->

    <owl:ObjectProperty rdf:about="http://ada.filmontology.org/ontology/2020/03/17/belongsToLevel">
        <owl:inverseOf rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/hasAnnotationType"/>
        <rdfs:domain rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationType"/>
        <rdfs:range rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationLevel"/>
        <rdfs:comment xml:lang="en">This object property connects annotation types to their superordinate annotation level.</rdfs:comment>
        <rdfs:label xml:lang="en">belongsToLevel</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/belongsToType -->

    <owl:ObjectProperty rdf:about="http://ada.filmontology.org/ontology/2020/03/17/belongsToType">
        <owl:inverseOf rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/hasPredefinedValue"/>
        <rdfs:domain rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationValue"/>
        <rdfs:range rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationType"/>
        <rdfs:comment xml:lang="en">This object property connects annotation values to their superordinate annotation type.</rdfs:comment>
        <rdfs:label xml:lang="en">belongsToType</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/hasAnnotationType -->

    <owl:ObjectProperty rdf:about="http://ada.filmontology.org/ontology/2020/03/17/hasAnnotationType">
        <rdfs:domain rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationLevel"/>
        <rdfs:range rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationType"/>
        <rdfs:comment xml:lang="en">This object property is used to associate all subordinate annotation types to an annotation level.</rdfs:comment>
        <rdfs:label xml:lang="en">hasAnnotationType</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/hasPredefinedValue -->

    <owl:ObjectProperty rdf:about="http://ada.filmontology.org/ontology/2020/03/17/hasPredefinedValue">
        <rdfs:domain rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationType"/>
        <rdfs:range rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationValue"/>
        <rdfs:comment xml:lang="en">This object property is used to associate all subordinate annotation level to an annotation type.</rdfs:comment>
        <rdfs:label xml:lang="en">hasPredefinedValue</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/mediaUri -->

    <owl:ObjectProperty rdf:about="http://ada.filmontology.org/ontology/2020/03/17/mediaUri">
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Film"/>
        <rdfs:range rdf:resource="http://schema.org/VideoObject"/>
        <rdfs:comment xml:lang="en">This media URI object property links a video file object to the movie metadata.</rdfs:comment>
        <rdfs:label xml:lang="en">mediaUri</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/adveneColorCode -->

    <owl:DatatypeProperty rdf:about="http://ada.filmontology.org/ontology/2020/03/17/adveneColorCode">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationType"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Holds the background color of annotation types in Advene.</rdfs:comment>
        <rdfs:label>adveneColorCode</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/annotationNumericValue -->

    <owl:DatatypeProperty rdf:about="http://ada.filmontology.org/ontology/2020/03/17/annotationNumericValue">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationValue"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Holds a numeric representation of an annotation value to support ordered annotation values.</rdfs:comment>
        <rdfs:label>annotationNumericValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/itemColor -->

    <owl:DatatypeProperty rdf:about="http://ada.filmontology.org/ontology/2020/03/17/itemColor">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationType"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Holds the Advene color function which determines the item color.</rdfs:comment>
        <rdfs:label>itemColor</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/prefixedLabel -->

    <owl:DatatypeProperty rdf:about="http://ada.filmontology.org/ontology/2020/03/17/prefixedLabel">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationType"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Holds the label of annotation types prefixed by the abbreviated level.</rdfs:comment>
        <rdfs:label>prefixedLabel</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/representation -->

    <owl:DatatypeProperty rdf:about="http://ada.filmontology.org/ontology/2020/03/17/representation">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationType"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Holds the Advene representation function for an annotation type.</rdfs:comment>
        <rdfs:label>representation</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/sequentialNumber -->

    <owl:DatatypeProperty rdf:about="http://ada.filmontology.org/ontology/2020/03/17/sequentialNumber">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationLevel"/>
        <rdfs:domain rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationType"/>
        <rdfs:domain rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationValue"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment>Holds an index value for annotation level, types, and values, to preserve the original order.</rdfs:comment>
        <rdfs:label>sequentialNumber</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/textAnnotationValue -->

    <owl:DatatypeProperty rdf:about="http://ada.filmontology.org/ontology/2020/03/17/textAnnotationValue">
        <rdfs:domain rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/FreeTextAnnotationType"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The text annotation value property is used to associate string value to a concrete movie annotation.</rdfs:comment>
        <rdfs:label xml:lang="en">textAnnotationValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/AdaCorpusAnalysis -->

    <owl:Class rdf:about="http://ada.filmontology.org/ontology/2020/03/17/AdaCorpusAnalysis">
        <rdfs:comment xml:lang="en">The set of base annotation types that are always annotated for a movie.</rdfs:comment>
        <rdfs:label xml:lang="en">Ada Corpus Analysis</rdfs:label>
    </owl:Class>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/AdaDetailedAnalysis -->

    <owl:Class rdf:about="http://ada.filmontology.org/ontology/2020/03/17/AdaDetailedAnalysis">
        <rdfs:subClassOf rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AdaCorpusAnalysis"/>
        <rdfs:comment xml:lang="en">The set of additional annotation types that may be annotated for some scenes of a movie in a more detailed analysis. It extends the set of types in AdA corpus analysis.</rdfs:comment>
        <rdfs:label xml:lang="en">Ada Detailed Analysis</rdfs:label>
    </owl:Class>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/AdaFilmontologyComplete -->

    <owl:Class rdf:about="http://ada.filmontology.org/ontology/2020/03/17/AdaFilmontologyComplete">
        <rdfs:subClassOf rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AdaDetailedAnalysis"/>
        <rdfs:comment xml:lang="en">The set of all annotation types available in the filmontology to be used for annotating audio-visual content. It extends the set of types in Ada Detailed Analysis.</rdfs:comment>
        <rdfs:label xml:lang="en">Ada Filmontology Complete</rdfs:label>
    </owl:Class>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/AnnotationLevel -->

    <owl:Class rdf:about="http://ada.filmontology.org/ontology/2020/03/17/AnnotationLevel">
        <rdfs:comment xml:lang="en">An annotation level is a category that groups a set of similar annotation types (e.g., all types related to camera or all types related to acoustics).</rdfs:comment>
        <rdfs:label xml:lang="en">Annotation Level</rdfs:label>
    </owl:Class>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/AnnotationType -->

    <owl:Class rdf:about="http://ada.filmontology.org/ontology/2020/03/17/AnnotationType">
        <rdfs:comment xml:lang="en">An annotation type refers to a concept of the annotation routine under which a movie is analyzed (e.g., camera movement speed, or dialogue intensity).</rdfs:comment>
        <rdfs:label xml:lang="en">Annotation Type</rdfs:label>
    </owl:Class>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/AnnotationValue -->

    <owl:Class rdf:about="http://ada.filmontology.org/ontology/2020/03/17/AnnotationValue">
        <rdfs:comment xml:lang="en">An annotation value is a concrete characteristic an annotation type can have (e.g., for camera movement speed - slow, medium, fast, alternating).</rdfs:comment>
        <rdfs:label xml:lang="en">Annotation Value</rdfs:label>
    </owl:Class>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/ContrastingAnnotationType -->

    <owl:Class rdf:about="http://ada.filmontology.org/ontology/2020/03/17/ContrastingAnnotationType">
        <rdfs:subClassOf rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationType"/>
        <rdfs:comment xml:lang="en">The contrasting annotation type connects two or more annotation values with a &quot;versus&quot; relationship (e.g., emotions of two persons: happy VS tensioned).</rdfs:comment>
        <rdfs:label xml:lang="en">Contrasting Annotation Type</rdfs:label>
    </owl:Class>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/EvolvingAnnotationType -->

    <owl:Class rdf:about="http://ada.filmontology.org/ontology/2020/03/17/EvolvingAnnotationType">
        <rdfs:subClassOf rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationType"/>
        <rdfs:comment xml:lang="en">The evolving annotation type describes the gradual transition of one annotation value to another value (e.g., camera movement speed: slow TO fast).</rdfs:comment>
        <rdfs:label xml:lang="en">Evolving Annotation Type</rdfs:label>
    </owl:Class>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/FreeTextAnnotationType -->

    <owl:Class rdf:about="http://ada.filmontology.org/ontology/2020/03/17/FreeTextAnnotationType">
        <rdfs:subClassOf rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationType"/>
        <rdfs:comment xml:lang="en">The free text annotation type can have arbitrary text values.</rdfs:comment>
        <rdfs:label xml:lang="en">Free Text Annotation Type</rdfs:label>
    </owl:Class>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/MultipleValueAnnotationType -->

    <owl:Class rdf:about="http://ada.filmontology.org/ontology/2020/03/17/MultipleValueAnnotationType">
        <rdfs:subClassOf rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationType"/>
        <rdfs:comment xml:lang="en">The multiple value annotation type allows to have more than one annotation value for one annotation.</rdfs:comment>
        <rdfs:label xml:lang="en">Multiple Value Annotation Type</rdfs:label>
    </owl:Class>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/NumericValuesAnnotationType -->

    <owl:Class rdf:about="http://ada.filmontology.org/ontology/2020/03/17/NumericValuesAnnotationType">
        <rdfs:subClassOf rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationType"/>
        <rdfs:comment xml:lang="en">The numeric value annotation type must have numeric annotation values.</rdfs:comment>
        <rdfs:label xml:lang="en">Numeric Value Annotation Type</rdfs:label>
    </owl:Class>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/PredefinedValuesAnnotationType -->

    <owl:Class rdf:about="http://ada.filmontology.org/ontology/2020/03/17/PredefinedValuesAnnotationType">
        <rdfs:subClassOf rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationType"/>
        <rdfs:comment xml:lang="en">The predefined value annotation type must use annotation values defined by the AdA ontology.</rdfs:comment>
        <rdfs:label xml:lang="en">Predefined Value Annotation Type</rdfs:label>
    </owl:Class>
    


    <!-- http://ada.filmontology.org/ontology/2020/03/17/SingleValueAnnotationType -->

    <owl:Class rdf:about="http://ada.filmontology.org/ontology/2020/03/17/SingleValueAnnotationType">
        <rdfs:subClassOf rdf:resource="http://ada.filmontology.org/ontology/2020/03/17/AnnotationType"/>
        <rdfs:comment xml:lang="en">The single value annotation type must have exactly one annotation value.</rdfs:comment>
        <rdfs:label xml:lang="en">Single Value Annotation Type</rdfs:label>
    </owl:Class>
    
