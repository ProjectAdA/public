        <annotationNumericValue rdf:datatype="http://www.w3.org/2001/XMLSchema#float">2.0</annotationNumericValue>
        <sequentialNumber>111</sequentialNumber>
        <rdfs:comment xml:lang="de">Die Kamera bewegt sich verhältnismäßig langsam.</rdfs:comment>
        <rdfs:comment xml:lang="en">The camera moves comparatively slow.</rdfs:comment>
